{
  "name": "simple-linalg",
  "version": "1.2.1",
  "description": "Simple linear algebra library for js",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "lib",
    "simple-linalg.d.ts"
  ],
  "types": "simple-linalg.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piercus/simple-linalg.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "keywords": [
    "linear",
    "algebra",
    "simple",
    "array",
    "kiss"
  ],
  "author": "Pierre Colle <piercus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piercus/simple-linalg/issues"
  },
  "homepage": "https://github.com/piercus/simple-linalg#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "ava": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "xo": "^0.54.1"
  },
  "xo": {
    "rules": {
      "no-multi-assign": 0,
      "import/extensions": 0,
      "unicorn/prefer-module": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-new-array": 0,
      "arrow-body-style": 0,
      "unicorn/no-array-callback-reference": 0,
      "valid-jsdoc": [
        1,
        {
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  },
  "dependencies": {
    "matrix-inverse": "^2.0.0"
  }
}
